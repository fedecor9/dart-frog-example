// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "dart run orm"
}

datasource db {
  provider = "postgresql"
  //This should be in an env file
  url      = "postgresql://db_user:123456@localhost:5432/dart_frog_example?schema=public"
}

model User {
   id   String @id @default(uuid())
   name String
   email String @unique
   password String
}